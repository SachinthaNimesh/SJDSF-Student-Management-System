{
  "expo": {
    "name": "Worky",
    "slug": "worky",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sachintha_wso2.studentmanagementapp",
      "versionCode": 6,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "a704672c-6fca-4644-b5d2-88c9dcf0662e"
      }
    },
    "runtimeVersion": "1.0.6",
    "updates": {
      "url": "https://u.expo.dev/a704672c-6fca-4644-b5d2-88c9dcf0662e"
    },
    "owner": "sjdsf",
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": "true"
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.sachintha-wso2.studentmanagementapp"
    }
  }
}
